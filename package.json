{
  "name": "jasmine-awesome-report",
  "version": "0.0.0-development",
  "description": "An awesome report for Jasmine",
  "main": "./src/awesome.js",
  "engines": {
    "node": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg && npm run lint",
    "lint": "eslint ./src/**/*.js",
    "build": "pkg-ok && npm run lint",
    "pretest": "npm run build",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release"
  },
  "author": "aperdomobo@gmail.com (aperdomob)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "chai": "^4.1.1",
    "commitizen": "^4.2.3",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^4.3.8",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "pkg-ok": "^1.1.0",
    "semantic-release": "^17.4.2",
    "sinon": "^9.2.3"
  },
  "dependencies": {
    "mochawesome-report-generator": "^5.2.0",
    "uuid": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aperdomob/jasmine-awesome-report.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
